/* pages/list/list.wxss */
.f2-chart {
  width: 100%;
  height: 500rpx;
}
.f2-chart-pie{
  width: 100%;
  height: 1200rpx;
}
.group {
  margin:60rpx 10rpx; 
}
.group .group-title {
  margin-bottom: 30rpx;
  font-size: 34rpx;
  font-weight: 700;
  color: #333;
}
.group .group-explain {
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #666;
}
.header-class {
  border: 1rpx solid #55C355;
  background-color: #55C355;
  color: #fff!important;
}
.header-class .td:first-child {
  color: #55C355;
}
.row-class:nth-child(2n) {
  background-color: #c1e4c175;
}